-- ----------------------------
-- Table structure for t_order
-- ----------------------------
DROP TABLE IF EXISTS `t_order`;
CREATE TABLE `t_order` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `sample_id` int(11) DEFAULT NULL COMMENT '样品id',
  `order_code` varchar(50) DEFAULT NULL COMMENT '订单code',
  `delivery_number` varchar(20) DEFAULT NULL COMMENT '交货号',
  `po_number` varchar(100) DEFAULT NULL COMMENT 'po号',
  `order_time` datetime DEFAULT NULL,
  `type` int(11) DEFAULT NULL COMMENT '1-自营；2-外贸;',
  `sample_code` varchar(50) DEFAULT NULL COMMENT '款号',
  `needle` varchar(100) DEFAULT NULL COMMENT '针形',
  `style` varchar(50) DEFAULT NULL COMMENT '款式',
  `status` int(11) DEFAULT '0' COMMENT '0-草稿；1-计划排期中；2-生产中；5-入库；-1-作废',
  `material` varchar(100) DEFAULT NULL COMMENT '材料',
  `company` varchar(100) DEFAULT NULL COMMENT '公司',
  `brand` varchar(50) DEFAULT NULL COMMENT '品牌',
  `photo_url` varchar(5000) DEFAULT NULL COMMENT '照片 以逗号分隔',
  `is_deleted` int(2) DEFAULT '0',
  `create_time` timestamp NULL DEFAULT NULL ON UPDATE CURRENT_TIMESTAMP,
  `update_time` timestamp NULL DEFAULT NULL ON UPDATE CURRENT_TIMESTAMP,
  `creater` int(11) DEFAULT NULL,
  `modifier` int(11) DEFAULT NULL,
  `remarks` varchar(1024) DEFAULT NULL,
  `winning_bid` int(2) DEFAULT '0' COMMENT '是否中标；0-否；1-是；(作废)',
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=36 DEFAULT CHARSET=utf8 COMMENT='入单 业务员接单';

