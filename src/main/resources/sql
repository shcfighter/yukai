  `inbound_date` datetime DEFAULT NULL COMMENT '生产日期',
  `status` int(2) DEFAULT '0' COMMENT '0-草稿；1-采购中；2-待入库；3-已入库；4-驳回；',
  `is_deleted` int(2) DEFAULT '0',
